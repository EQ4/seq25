<article class='instrument'>
  <dl>
    <dt><label for='volume'>v</label></dt>
    <dd>
      {{view 'range' id='volume' min='0' max='1' step='0.01' value=volume}}
    </dd>
    <dt><label for='attack'>a</label></dt>
    <dd>
      {{view 'range' id='attack' min='0' max='1' step='0.01' value=attack}}
    </dd>
    <dt><label for='resonance'>r</label></dt>
    <dd>
      {{view 'range' id='resonance' min='0' max='1' step='0.01' value=resonance}}
    </dd>
  </dl>
  <dl>
    <dt><label for='filterQ'>q</label></dt>
    <dd>
      {{view 'range' id='filterQ' min='0' max='1' step='0.01' value=filterQ}}
    </dd>
    <dt><label for='filterFreq'>f</label></dt>
    <dd>
      {{view 'range' id='filterFreq' min='0' max='1' step='0.01' value=filterFreq}}
    </dd>
    <dt><label for='shape'>shape</label></dt>
    <dd>
      {{view 'select' id='shape' content=shapes value=shape}}
    </dd>
  </dl>
</article>
