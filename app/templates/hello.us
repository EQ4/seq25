<table>
	<thead>
		<tr>
			<th></th>
			<% for(var beat = 1; beat <= beats; beat++) { %>
			<th><%= beat %></th>
			<% } %>
		</tr>
	</thead>
	<tbody>
	<% for(var i=0; i < notes.length; i++) { %>
	<tr>
		<th><%= notes[i].name %></th>
		<% for(var beat = 1; beat <= beats; beat++) { %>
		<td><a href="#toggle/<%= notes[i].number + '/' + beat %>">&nbsp;</a></td>
		<% } %>
	</tr>
	<%}%>
	</tbody>
</table>
