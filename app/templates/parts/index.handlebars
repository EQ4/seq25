<section id='parts'>
  <ul>
    {{#each row in rows}}
      <li>
        <ul class='part-row'>
          {{#each part in row}}
            <li class='part'>
              {{render 'parts/summary' part}}
            </li>
          {{/each}}
        </ul>
      </li>
    {{/each}}
  </ul>
</section>
