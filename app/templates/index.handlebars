<table>
  <thead>
    <tr>
      <th>
        {{view Seq25.TempoView min=0 max=200 value=tempo}}
      </th>
      {{#each beat in beats}}
        <th>{{beat}}</th>
      {{/each}}
    </tr>
  </thead>
  <tbody>
    {{#each pitch in pitches}}
      <tr {{bind-attr class=isSharp:sharp}}>
        {{#piano-key}}
          {{name}}
        {{/piano-key}}
        {{#each beat in beats}}
          <td {{bind-attr class=isOn:on }} {{action 'toggleNote'}} >
          </td>
        {{/each}}
      </tr>
    {{/each}}
  </tbody>
</table>
