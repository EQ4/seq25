{
  "app/js/models/*.coffee":      {"command": "model", "alternate": "spec/models/{}-spec.coffee"},
  "app/js/views/*.coffee":       {"command": "view"},
  "app/js/controllers/*.coffee": {"command": "controller"},
  "app/templates/*.handlebars":  {"command": "template"},
  "spec/*-spec.coffee":          {"command": "spec", "alternate": "app/js/{}.coffee"},
  "app/css/*.less":              {"command": "stylesheet"}
}
